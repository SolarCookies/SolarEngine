// area settings (override settings found in Maya file)

Version = 1;

[Lights]
{
	[directionalLight]
	{
		instanceName = global;
		Zone = -1;
		Type = 3;
		Color = 0.25 0.25 0.25;
		ShadowColor = 0.25 0.25 0.25;
		Intensity = 1.0;
		Direction = 0.0  -1 0.0;
		LightObjects = true;
		CastRTWorldShadows = true;
	}
}

[Render]
{
	RenderAlphaNodesFirst = false;
}


